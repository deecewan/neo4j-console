{
  "name": "neo4j-console",
  "version": "0.1.1",
  "description": "A web and command line interface for the Neo4j Graph Database",
  "main": "src/index.js",
  "dependencies": {
    "express": "^4.14.0",
    "neo4j-driver": "^1.0.2",
    "config": "^1.21.0"
  },
  "devDependencies": {
    "eslint": "^3.9.1",
    "forever": "^0.15.3",
    "mocha": "^3.2.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "./node_modules/.bin/eslint 'src/.' --ext '.js'",
    "test": "npm run lint; ./node_modules/mocha/bin/mocha --harmony"
    "test:start": "./node_modules/.bin/forever start src/index.js",
    "test:stop": "./node_modules/.bin/forever stop src/index.js",
    "test:clean-logs": "./node_modules/.bin/forever cleanlogs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itaisteinherz/neo4j-console.git"
  },
  "keywords": [
    "neo4j",
    "node.js",
    "javascript"
  ],
  "author": "Itai Steinherz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itaisteinherz/neo4j-console/issues"
  },
  "homepage": "https://github.com/itaisteinherz/neo4j-console#readme"
}
